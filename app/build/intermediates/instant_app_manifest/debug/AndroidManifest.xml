<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.infrolog.elu.android"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="1" />

    <application
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:testOnly="true" >
        <activity
            android:name="com.infrolog.elu.android.Sudoku"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.infrolog.elu.android.About"
            android:label="@string/about_title"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name="com.infrolog.elu.android.Prefs"
            android:label="@string/settings_title" />
        <activity
            android:name="com.infrolog.elu.android.Game"
            android:label="@string/game_title" />
    </application>

</manifest>